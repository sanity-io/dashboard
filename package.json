{
  "name": "@sanity/dashboard",
  "version": "3.1.6",
  "description": "Tool for rendering dashboard widgets",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "dashboard",
    "sanity-plugin",
    "sanity-tool"
  ],
  "homepage": "https://github.com/sanity-io/dashboard#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/dashboard/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/dashboard.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "source": "./src/index.ts",
      "import": "./lib/index.esm.js",
      "require": "./lib/index.js",
      "default": "./lib/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.esm.js",
  "source": "./src/index.ts",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "prebuild": "npm run clean && plugin-kit verify-package --silent && pkg-utils",
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf lib",
    "compile": "tsc --noEmit",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/icons": "^3.3.0",
    "@sanity/image-url": "^1.0.2",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^2.6.3",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@sanity/pkg-utils": "^2.4.10",
    "@sanity/plugin-kit": "^3.1.12",
    "@sanity/semantic-release-preset": "^5.0.0",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "prettier-plugin-packagejson": "^2.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "rimraf": "^6.0.0",
    "sanity": "^3.49.0",
    "styled-components": "^6.1.11",
    "typescript": "5.1.3"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3",
    "styled-components": "^6.1"
  },
  "engines": {
    "node": ">=18"
  },
  "sanityExchangeUrl": "https://www.sanity.io/exchange/dashboard"
}
